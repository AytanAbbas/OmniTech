<script>
    $(document).ready(function () {
        var url = window.location.href;
        $('.navbar-nav .nav-link').removeClass('active');
        $('.navbar-nav .nav-link').filter(function () {
            return url.indexOf($(this).attr('href')) != -1;
        }).addClass('active');
    });
</script>






